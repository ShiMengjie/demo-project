<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.data.statistic.dao.mapper.DataOptionMapper">

    <select id="selectOneByKey"
            resultType="com.example.demo.data.statistic.po.DataOption">
        SELECT *
        FROM data_option
        WHERE `key` = #{key};
    </select>
</mapper>